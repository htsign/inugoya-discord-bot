{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "node プログラムの起動",
      "skipFiles": ["<node_internals>/**"],
      "args": ["--env-file=${workspaceFolder}/.env", "${workspaceFolder}/index.ts"],
    },
    {
      "type": "bun",
      "request": "launch",
      "name": "bun プログラムの起動",
      "program": "${workspaceFolder}/index.ts",
    }
  ]
}
